class Student:

    # class attribute
    name = ""
    age = 21
# create std object
std = Student()
std.name = "Bharat"
std.age = 10

# create another object std2
std2 = Student()
std2.name = "Ajay"
std2.age = 15

# access attributes
print(f"{std.name} is {std.age} years old")
print(f"{std2.name} is {std2.age} years old")
